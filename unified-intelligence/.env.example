# Redis Configuration
# Copy this file to .env and set your actual values

# Redis server host (default: 127.0.0.1)
REDIS_HOST=192.168.1.160

# Redis server port (default: 6379)
REDIS_PORT=6379

# Redis password (leave empty if no password)
REDIS_PASSWORD=legacymind_redis_pass

# Redis database number (default: 0)
REDIS_DB=0

# Instance identifier for this MCP server
INSTANCE_ID=test

# Input Validation Limits
# Maximum thought content length in characters (default: 10000)
MAX_THOUGHT_LENGTH=10000

# Maximum thoughts per chain (default: 1000)
MAX_THOUGHTS_PER_CHAIN=1000